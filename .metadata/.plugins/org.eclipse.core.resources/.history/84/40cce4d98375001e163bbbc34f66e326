package Collection_Day1;

import java.util.ArrayList;
import java.util.Collection;

public class Demo1 {
	public static void main(String[] args) {
		Collection c = new ArrayList();
		c.add("abcd");
		c.add(101);
		c.add(10.67);
		c.add(true)
		System.out.println(c);
		System.out.println(c.size());
	}
}
